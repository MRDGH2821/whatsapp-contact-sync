{
  "tasks": [
    {
      "isBackground": true,
      "label": "web: dev",
      "name": "Website (dev mode)",
      "options": {
        "cwd": "${workspaceFolder}/web"
      },
      "problemMatcher": {
        "background": {
          "activeOnStart": true,
          "beginsPattern": ".*vite.*",
          "endsPattern": "Local:.*http://localhost:4000.*"
        },
        "pattern": {
          "regexp": "http://localhost:4000/"
        }
      },
      "script": "dev",
      "type": "npm"
    }
  ],
  "version": "2.0.0"
}
