{
  "version": "2.0.0",
  "tasks": [
    {
      "isBackground": true,
      "type": "npm",
      "script": "dev",
      "options": {
        "cwd": "${workspaceFolder}/web"
      },
      "problemMatcher": {
        "background": {
          "activeOnStart": true,
          "beginsPattern": ".*vite.*",
          "endsPattern": "Local:.*http://localhost:4000.*"
        }
      }
    }
  ]
}
